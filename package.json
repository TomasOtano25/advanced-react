{
  "name": "advanced-react",
  "version": "0.0.1",
  "description": "Proyecto Isomorphic React",
  "main": "lib/server.js",
  "scripts": {
    "dev": "set NODE_PATH=./lib&&  nodemon --exec babel-node lib/server.js",
    "start-prod": "set NODE_ENV=production set NODE_PATH=./lib&& pm2 start build/server.js -i max --name appProd",
    "start": "set NODE_ENV=production set NODE_PATH=./lib&& node build/server.js",
    "webpack": "webpack -wd",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "build": "npm-run-all build-*",
    "test": "jest --watch",
    "lint": "eslint .",
    "verify-tests": "jest --coverage"
  },
  "author": "Tomas Garia Ota√±o (tomasotano25@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6",
    "webpack": "^4.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^1.0.0-rc.4",
    "jest": "^22.4.3",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.3",
    "pm2": "^2.10.3",
    "react-test-renderer": "^16.3.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./lib/__test__/setup.js"
    ]
  },
  "now": {
    "engines": {
      "node": "9.6.1"
    }
  }
}
